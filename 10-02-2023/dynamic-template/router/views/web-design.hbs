<html lang='it'>

  {{> head}}

  <body>
    <div class='container-fluid'>
      {{> header}}

      <!--<div id="img-cover">
            <img src="images/4.jpg" class="img-fluid" alt="">
        </div>-->

      <!-- Immagine in background con testo sovrascritto-->
      <div
        id='img-cover-contatti'
        class='d-flex justify-content-center align-items-center'
      >
        <div id='text-cover'>
          <h2>WEB DESIGN</h2>
          <p class='lead text-center'>Sviluppo web</p>
        </div>

      </div>

    </div>

    <div class='container-fluid'>

      <!--BREADCRUMB-->
      <div class='mt-3'>
        <nav aria-label='breadcrumb' id='bc-box'>
          <ol class='breadcrumb'>
            <li class='breadcrumb-item' aria-current='page'><a
                href='index.html'
              >Home</a></li>
            <li class='breadcrumb-item active' aria-current='page'>Servizi/Web
              Design</li>
          </ol>
        </nav>
      </div>

      <!--TITOLO DEL CONTENUTO-->
      <div class='d-flex justify-content-center'>
        <h1 class='display-2 my-5'>Modern web Design</h1>
      </div>

      <!-- SLIDESHOW-->
      <div
        class='carousel slide mb-5'
        data-bs-ride='carousel'
        id='firstCarousel'
      >

        <!-- Indicatori Slide -->
        <div class='carousel-indicators'>
          <button
            type='button'
            data-bs-target='#firstCarousel'
            data-bs-slide-to='0'
            class='active'
          ></button>
          <button
            type='button'
            data-bs-target='#firstCarousel'
            data-bs-slide-to='1'
          ></button>
          <button
            type='button'
            data-bs-target='#firstCarousel'
            data-bs-slide-to='2'
          ></button>
          <button
            type='button'
            data-bs-target='#firstCarousel'
            data-bs-slide-to='3'
          ></button>
        </div>

        <div class='carousel-inner'>
          <div class='carousel-item active'>
            <img src='images/1.jpg' class='w-100' />
            <div class='carousel-caption d-none d-md-block'>
              <h3>Prima slide</h3>
              <p>Sottotitolo della prima slide</p>
            </div>
          </div>

          <div class='carousel-item'>
            <img src='images/2.jpg' class='w-100' />
            <div class='carousel-caption d-none d-md-block'>
              <h3>Seconda slide</h3>
              <p>Sottotitolo della seconda slide</p>
            </div>
          </div>

          <div class='carousel-item'>
            <img src='images/3.jpg' class='w-100' />
            <div class='carousel-caption d-none d-md-block'>
              <h3>Terza slide</h3>
              <p>Sottotitolo della terza slide</p>
            </div>
          </div>

          <div class='carousel-item'>
            <img src='images/5.jpg' class='w-100' />
            <div class='carousel-caption d-none d-md-block'>
              <h3>Quarta slide</h3>
              <p>Sottotitolo della Quarta slide</p>
            </div>
          </div>
        </div>
        <!-- Controlli slide -->

        <button
          type='button'
          class='carousel-control-prev'
          data-bs-target='#firstCarousel'
          data-bs-slide='prev'
        >
          <span class='carousel-control-prev-icon'></span>
          <span class='visually-hidden'>Prima</span>
        </button>

        <button
          type='button'
          class='carousel-control-next'
          data-bs-target='#firstCarousel'
          data-bs-slide='next'
        >
          <span class='carousel-control-next-icon'></span>
          <span class='visually-hidden'>Dopo</span>
        </button>

      </div>

      <div class='d-flex justify-content-center'>
        <h1 class='display-2 my-5'>My Skills</h1>
      </div>

      <!-- COMPONENTE TABS-->

      <section>

        <div class='d-flex flex-column w-50 mb-5 mx-auto'>
          <!-- STRUTTURA -->
          <ul class='nav nav-tabs'>
            <li class='nav-item'>
              <button
                class='nav-link'
                type='button'
                data-bs-toggle='tab'
                data-bs-target='#react'
              >React</button>
            </li>

            <li class='nav-item'>
              <button
                class='nav-link'
                type='button'
                data-bs-toggle='tab'
                data-bs-target='#vue'
              >Vue</button>
            </li>

            <li class='nav-item'>
              <button
                class='nav-link'
                type='button'
                data-bs-toggle='tab'
                data-bs-target='#angular'
              >Angular</button>
            </li>
          </ul>

          <div class='tab-content mt-3'>
            <div class='tab-pane fade show active p-3' id='react'>
              <p>REACT - Lorem ipsum dolor sit amet ac facilisi orci vestibulum
                orci sodales.</p>
            </div>
            <div class='tab-pane fade p-3' id='vue'>
              <p>Vue - Lorem ipsum dolor sit amet ac facilisi orci vestibulum
                orci sodales.</p>
            </div>
            <div class='tab-pane fade p-3' id='angular'>
              <p>ANGULAR - Lorem ipsum dolor sit amet ac facilisi orci
                vestibulum orci sodales.</p>
            </div>

          </div>

        </div>

      </section>

    </div>
    <!--Chiusura div container-->

    {{> footer}}

    <!--SCRIPT JAVASCRIPT-->
    {{> script}}
  </body>

</html>